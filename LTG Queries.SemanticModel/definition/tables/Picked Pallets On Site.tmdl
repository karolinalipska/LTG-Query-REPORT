	partition 'Picked Pallets On Site' = m
		mode: directQuery
		source =
				let
				    Source = Oracle.Database(LTG_BlueYonder_Server_Prod, [HierarchicalNavigation=true, Query="select #(lf)iv.STOLOC ""Location"",#(lf)dm.lngdsc ""Area"",#(lf)iv.LODNUM ""LPN"",#(lf)iv.PRTNUM ""SKU"",#(lf)case#(lf)    when iv.PRT_CLIENT_ID = 'CID000001' then 'Mars'#(lf)    when iv.PRT_CLIENT_ID = 'CID013634' then 'Mondelez'#(lf)    else iv.PRT_CLIENT_ID#(lf)    end ""Client"",#(lf)iv.LOTNUM ""Batch"",#(lf)iv.INV_ATTR_STR2 ""SAP Storage"", --25/09/25 added by request 4990 --#(lf)iv.EXPIRE_DTE ""Expiry"",#(lf)iv.FTPCOD ""Footprint"",#(lf)pfd.untqty  ""Footprint Qty of SKU"",#(lf)--iv.SHIP_LINE_ID ,#(lf)sl.ordnum   ""Order Number"",#(lf)spv.car_move_id     ""Load ID"",#(lf)iv.untqty    ""Qty"",#(lf)--iv.WRKREF ,#(lf)--iv.WRKREF_DTL ,#(lf)iv.LSTMOV-1/24   ""Last Movement"",#(lf)spv.pckdte-1/24 ""Picked Date"" --25/09/25 added by request 4990 --#(lf)from all_inventory_view iv#(lf)left join locmst lm on iv.stoloc = lm.stoloc#(lf)left join shipment_line sl on iv.ship_line_id =sl.ship_line_id#(lf)left join prtftp_dtl pfd on iv.prtnum = pfd.prtnum and iv.prt_client_id = pfd.prt_client_id and iv.ftpcod = pfd.ftpcod and pfd.wh_id = 'GB_5024' and pfd.uomcod = 'PA'#(lf)left join shipping_pckwrk_view spv on iv.wrkref_dtl = spv.wrkref_dtl#(lf)left join dscmst dm#(lf)on lm.arecod||'|GB_5024' = dm.colval and dm.colnam = 'arecod|wh_id'#(lf)#(lf)where iv.ship_line_id is not null#(lf)and lm.arecod not in ('DSPCH')"])
				in
				    Source

