	partition 'Picked Pallets On Site' = m
		mode: directQuery
		source =
				let
				    Source = Oracle.Database(LTG_BlueYonder_Server_Prod, [HierarchicalNavigation=true, Query="SELECT#(lf)    iv.stoloc           ""Location"",#(lf)    dm.lngdsc           ""Area"",#(lf)    iv.lodnum           ""LPN"",#(lf)    iv.prtnum           ""SKU"",#(lf)    CASE#(lf)        WHEN iv.prt_client_id = 'CID000001' THEN#(lf)            'Mars'#(lf)        WHEN iv.prt_client_id = 'CID013634' THEN#(lf)            'Mondelez'#(lf)        ELSE#(lf)            iv.prt_client_id#(lf)    END                 ""Client"",#(lf)    iv.lotnum           ""Batch"",#(lf)    BS.""Business Segment"",#(lf)    iv.inv_attr_str2    ""SAP Storage"", --25/09/25 added by request 4990 --#(lf)    iv.expire_dte       ""Expiry"",#(lf)    iv.ftpcod           ""Footprint"",#(lf)    pfd.untqty          ""Footprint Qty of SKU"",#(lf)--iv.SHIP_LINE_ID ,#(lf)    sl.ordnum           ""Order Number"",#(lf)    spv.car_move_id     ""Load ID"",#(lf)    iv.untqty           ""Qty"",#(lf)--iv.WRKREF ,#(lf)--iv.WRKREF_DTL ,#(lf)    iv.lstmov - 1 / 24  ""Last Movement"",#(lf)    spv.pckdte - 1 / 24 ""Picked Date"", --25/09/25 added by request 4990 --#(lf)    case when trunc(spv.pckdte - 1 / 24)>= trunc(sysdate-1/24) - 10 then 'In Last 10 Days' else 'Over 10 Days Ago' end ""When Picked"" -- KL 05/11/2025 added by request 05786 --#(lf)FROM#(lf)    all_inventory_view   iv#(lf)    LEFT JOIN locmst               lm ON iv.stoloc = lm.stoloc#(lf)    LEFT JOIN shipment_line        sl ON iv.ship_line_id = sl.ship_line_id#(lf)    LEFT JOIN prtftp_dtl           pfd ON iv.prtnum = pfd.prtnum#(lf)                                AND iv.prt_client_id = pfd.prt_client_id#(lf)                                AND iv.ftpcod = pfd.ftpcod#(lf)                                AND pfd.wh_id = 'GB_5024'#(lf)                                AND pfd.uomcod = 'PA'#(lf)    LEFT JOIN shipping_pckwrk_view spv ON iv.wrkref_dtl = spv.wrkref_dtl#(lf)    LEFT JOIN dscmst               dm ON lm.arecod || '|GB_5024' = dm.colval#(lf)                           AND dm.colnam = 'arecod|wh_id'#(lf)    LEFT JOIN (#(lf)        SELECT#(lf)            pm.prtnum,#(lf)            pm.prt_client_id,#(lf)            pm.wh_id_tmpl,#(lf)            pm.prt_class_name,#(lf)            dm1.lngdsc ""Business Segment""#(lf)        FROM#(lf)            prtmst pm#(lf)            LEFT JOIN dscmst dm1 ON pm.prt_class_name = dm1.colval#(lf)                                    AND dm1.colnam = 'prt_class_name'#(lf)        WHERE#(lf)            pm.wh_id_tmpl = 'GB_5024'#(lf)    )                    bs ON iv.prtnum = bs.prtnum#(lf)            AND iv.prt_client_id = bs.prt_client_id#(lf)            AND iv.wh_id = bs.wh_id_tmpl#(lf)WHERE#(lf)    iv.ship_line_id IS NOT NULL#(lf)    AND lm.arecod NOT IN ( 'DSPCH' )"])
				in
				    Source

