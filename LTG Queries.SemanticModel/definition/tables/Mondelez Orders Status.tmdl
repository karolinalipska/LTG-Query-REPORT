	partition 'Mondelez Orders Status' = m
		mode: directQuery
		source =
				let
				    Source = Oracle.Database(LTG_BlueYonder_Server_Prod, [HierarchicalNavigation=true, Query="SELECT#(lf)    o.ordnum                        ""Order Number"",#(lf)    o.entdte - 1 / 24               ""Order Entry Date"",#(lf)    CASE#(lf)        WHEN ( SUM(ol.ordqty) - SUM(ol.shpqty) = 0 ) THEN#(lf)            'Shipped in Full'#(lf)        WHEN ( SUM(ol.ordqty) - SUM(ol.shpqty) > 0#(lf)               AND SUM(ol.ordqty) - SUM(ol.shpqty) < SUM(ol.ordqty) )#(lf)             AND ol.cancelled_flg = 1 THEN#(lf)            'Shipped Partially - Cancelled'#(lf)        WHEN ( SUM(ol.ordqty) - SUM(ol.shpqty) > 0#(lf)               AND SUM(ol.ordqty) - SUM(ol.shpqty) < SUM(ol.ordqty) )#(lf)             AND ol.cancelled_flg = 0 THEN#(lf)            'Shipped Partially - Active'#(lf)        WHEN ( SUM(ol.ordqty) - SUM(ol.shpqty) = SUM(ol.ordqty) )#(lf)             AND ol.cancelled_flg = 1 THEN#(lf)            'Not Shipped - Cancelled'#(lf)        WHEN ( SUM(ol.ordqty) - SUM(ol.shpqty) = SUM(ol.ordqty) )#(lf)             AND ol.cancelled_flg = 0 THEN#(lf)            'Not Shipped - Active'#(lf)    END                             ""Order Fulfilment - Order Status"",#(lf)    SUM(ol.ordqty) - SUM(ol.shpqty) ""Shorts"",#(lf)    dm.lngdsc                       ""Shipment Line Status"",#(lf)    sl.schbat                       ""Wave"",#(lf)    dm1.lngdsc                      ""Shipment Status"",#(lf)    cm.car_move_id                  ""Load ID"",#(lf)    cm.host_ext_id                  ""Host External Id"",#(lf)    a.start_dte - 1 / 24            ""Appointment Start"",#(lf)    t.trlr_num                      ""Trailer Assigned"",#(lf)    t.close_dte - 1 / 24            ""Trailer Closed"",#(lf)    t.dispatch_dte - 1 / 24         ""Trailer Despatched""#(lf)FROM#(lf)    ord           o#(lf)    LEFT JOIN ord_line      ol ON o.ordnum = ol.ordnum#(lf)    LEFT JOIN shipment_line sl ON ol.ordnum || ol.ordlin = sl.ordnum || sl.ordlin#(lf)    LEFT JOIN dscmst        dm ON sl.linsts = dm.colval#(lf)                           AND dm.colnam = 'linsts'#(lf)    LEFT JOIN shipment      s ON sl.ship_id = s.ship_id#(lf)    LEFT JOIN dscmst        dm1 ON s.shpsts = dm1.colval#(lf)                            AND dm1.colnam = 'shpsts'#(lf)    LEFT JOIN stop          st ON s.stop_id = st.stop_id#(lf)    LEFT JOIN car_move      cm ON st.car_move_id = cm.car_move_id#(lf)    LEFT JOIN appt          a ON cm.car_move_id = a.car_move_id#(lf)    LEFT JOIN trlr          t ON cm.trlr_id = t.trlr_id#(lf)    LEFT JOIN pckbat        pb ON sl.schbat = pb.schbat#(lf)WHERE#(lf)    o.client_id = 'CID013634'#(lf)GROUP BY#(lf)    o.ordnum,#(lf)    ol.cancelled_flg,#(lf)    o.entdte - 1 / 24,#(lf)    dm.lngdsc,#(lf)    sl.schbat,#(lf)    dm1.lngdsc,#(lf)    cm.car_move_id,#(lf)    cm.host_ext_id,#(lf)    a.start_dte - 1 / 24,#(lf)    t.trlr_num,#(lf)    t.close_dte - 1 / 24,#(lf)    t.dispatch_dte - 1 / 24#(lf)ORDER BY#(lf)    2"])
				in
				    Source

